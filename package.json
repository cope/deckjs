		"cards"
	],
	"author": "Predrag Stojadinovic",
	"license": "Unlicense",
	"bugs": {
		"url": "https://github.com/cope/preferans-deck-js/issues"
	},
	"homepage": "https://github.com/cope/preferans-deck-js#readme",
	"dependencies": {
		"lodash": "^4.17.10"
	},
	"devDependencies": {
		"chai": "^4.1.2",
		"coveralls": "^3.0.1",
		"mocha": "^5.1.1",
		"nyc": "^12.0.2",
		"sonarjs": "^1.0.0"
	}
}
