{
	"name": "preferans-deck-js",
	"version": "1.1.3",
	"description": "nodejs deck of cards for preferans",
	"engines": {
		"node": ">=8.9"
	},
	"main": "lib/deck.js",
	"scripts": {
		"test": "nyc --reporter=lcov --reporter=html mocha",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"sonar": "sonarjs -e .sonarlint/**,node_modules/**,coverage/**,.nyc_output/**,_ref/**"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cope/preferans-deck-js.git"
	},
	"keywords": [
